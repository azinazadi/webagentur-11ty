                "@type": "Brand",
                "name": "Webagentur Berlin"
            },
            "offers": {
                "@type": "Offer",
                "priceCurrency": "USD",
                "price": "5000",
                "url": "https://webagenturberlin.com",
                "availability": "https://schema.org/InStock",
                "itemCondition": "https://schema.org/NewCondition",
                "priceValidUntil": "2023-12-31"
            },
            "aggregateRating": {
                "@type": "AggregateRating",
                "ratingValue" : "4.8",
                "ratingCount" : "30",
                "reviewCount" : "18"

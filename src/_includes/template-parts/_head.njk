            "@context": "http://schema.org/",
            "@type": "Organization",
            "name": "Webagentur Berlin",
            "logo": "https://webagenturberlin.com/public/logo.png",
            "url": "https://webagenturberlin.com",
            "address": {
                "@type": "PostalAddress",
                "streetAddress": "Elsenstr.",
